 		"http://mybatis.org/dtd/mybatis-3-config.dtd">
  						
<configuration>

	<typeAliases>
		<typeAlias alias="User" type="board.raw.domain.User" />
		<typeAlias alias="Board" type="board.raw.domain.Board" />
		<typeAlias alias="Article" type="board.raw.domain.Article" />
