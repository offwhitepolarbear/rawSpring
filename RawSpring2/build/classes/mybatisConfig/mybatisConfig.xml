		<typeAlias alias="Search" type="common.domain.Search" />
		<typeAlias alias="ArticleSearch" type="common.domain.ArticleSearch" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/userMapper.xml"/>
		<mapper resource="mapper/boardMapper.xml"/>
